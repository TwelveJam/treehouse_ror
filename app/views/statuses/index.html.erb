<h1>Forum</h1>

<h2>Tweets</h2>

<table>
 
<% @statuses.each do |status| %>
  <tr>
    <td><%= status.name %></td>
    <td><%= status.content %></td>
    <td><%= link_to 'Edit', edit_status_path(status) %></td>
    <td><%= link_to 'Delete', status, method: :delete, data: { confirm: 'Do Not Leave Me!?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Make A Status', new_status_path %>
